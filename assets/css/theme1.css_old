.btn.btn-xs.btn_white_table{height: 24px;  border-radius: 5px;  box-shadow: 0 1px 1px 0 rgba(0, 36, 128, 0.07);  background-color: #ffffff;  border: solid 1px rgba(217, 224, 238, 0.5); padding: 3px 10px 3px 10px; font-size: 10px; font-weight: 300; color: #6d788e;}
