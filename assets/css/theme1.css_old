/*******************************
PANEL CSS
**********/
.panel{color: #1d2129 !important; box-shadow: none!important; border: none!important; border-radius: 5px; }
.panel-heading{border-bottom: 1px solid #f5f4f5!important;	position: relative;	border-top-right-radius: 5px;	border-top-left-radius: 5px; box-shadow: 0 3px 2px 0 rgba(0, 53, 193, 0.01), 0 1px 1px 0 rgba(0, 19, 151, 0.5); min-height: 66px;}

.panel-footer{border-top: 1px solid #f5f4f5!important;	position: relative;	border-bottom-left-radius: 5px;	border-bottom-right-radius: 5px; box-shadow:none!important;}



.panel-heading > .panel-title {  font-weight: 500!important; font-size: 14px!important; color: #1e1e40; letter-spacing: 0.1px!important}
.heading-elements > .panel-title {  font-weight: 400!important; font-size: 14px!important; color: #1e1e40!important; letter-spacing: 0.1px!important; margin-left: 15px;}
.heading-elements > .panel-title i, .panel-heading > .panel-title i{font-size: 12px;}
.heading-elements > .panel-title a, .panel-heading > .panel-title a {  font-weight: 500!important; font-size: 13px!important; color: #404b66!important; letter-spacing: 0.1px!important; }
.heading-elements > .panel-title a i, .panel-heading > .panel-title a i{font-size: 12px; color: #1e1e40!important;}
.heading-elements > .panel-title a:hover, .panel-heading > .panel-title a:hover{color: #102243!important}
.heading-elements > .panel-title a.active, .panel-heading > .panel-title a.active{color: #102243!important}

.panel-group-control .panel-title > a.collapsed::before {
	content: '\e9c1';
}
.panel-group-control .panel-title > a::before {
	content: '\e9c2';
	color: #c4c4e1;
	font-size: 13px;
}



.panel-flat > .panel-heading + .panel-body {	padding-top: 20px;}
.panel-body{background-image: linear-gradient(252deg, #f7f8fb, #ffffff); box-shadow: 0 3px 2px 0 rgba(0, 50, 179, 0.03), 0 1px 1px 0 rgba(0, 17, 136, 0.08); border:none!important; border-bottom-right-radius: 5px;	border-bottom-left-radius: 5px;  }
@media (max-width: 1600px) {
.panel {color: #1d2129 !important;overflow: auto;}
}

