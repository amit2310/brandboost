/*******************************
BUTTONS
*/
.btn i{font-size: 12px!important;}
.btn.dark_btn {	display:inline-block;	border-radius: 5px;	font-size: 13px !important;	background: #1e1e40;	border:none;	height: 52px;	padding: 7px 20px;	font-weight: 400;	color: #fff;
 box-shadow: 0 2px 1px 0 rgba(0, 0, 0, 0.2), inset 0 1px 0 0 rgba(255, 255, 255, 0.05), inset 0 -1px 0 0 rgba(0, 0, 0, 0.05);}
