.btn.btn-xs.btn_white_table:hover{ background-color: #f0f2f8!important;  border: solid 1px #e3e9f3!important;}


.btn-link{color: #6a7995!important; font-weight: 400!important;}

.btn_purple{background-image: linear-gradient(261deg, #8765ed, #6145d4)!important;  box-shadow: 0 2px 1px 0 rgba(102, 73, 215, 0.2), inset 0 1px 0 0 rgba(255, 255, 255, 0.05), inset 0 -1px 0 0 rgba(0, 0, 0, 0.05)!important;}

.btn.white_btn {	display:inline-block;	border-radius: 5px;	font-size: 13px !important;	height: 52px;	padding: 7px 20px;	font-weight: 400;	box-shadow: 0 1px 1px 0 rgba(0, 36, 128, 0.08), 0 0 1px 0 rgba(0, 0, 0, 0.03);  background-color: #ffffff;  border: solid 1px #e3e9f3; color: #102243;}
