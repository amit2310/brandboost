.btn.light_btn {display:inline-block;	border-radius: 5px;	font-size: 13px !important;	background-color: rgba(255, 255, 255, 0.20);	border:none;	height: 52px;	padding: 7px 20px;	font-weight: 400;	color: #fff;    box-shadow: 0 2px 1px 0 rgba(0, 0, 0, 0.1), inset 0 -1px 0 0 rgba(0, 0, 0, 0.03), inset 0 1px 0 0 rgba(255, 255, 255, 0.01); }
